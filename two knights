#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    for (int k = 1; k <= n; ++k) {
        // Total ways to place two knights on a k x k board
        long long total_positions = (long long)k * k * (k * k - 1) / 2;

        // Number of positions where knights attack each other
        long long attacking_positions = 4 * (k - 1) * (k - 2);

        // Non-attacking positions
        long long result = total_positions - attacking_positions;

        cout << result << endl;
    }

    return 0;
}

