#include<bits/stdc++.h>
using namespace std;
int main(){
    long long  n;
    cin>>n;
    string str="";
    if(n>1 && n<=3){
        cout<<"NO SOLUTION";
    }
    else if(n==1){
        cout<<"1";
    }
    else{
        for(long long i=1;i<=n;i++){
            if(i%2==0){
                cout<<i<<" ";
            }
            else{
                str+=to_string(i)+" ";
            }
        }
    }
    cout<<str;
return 0;
}
